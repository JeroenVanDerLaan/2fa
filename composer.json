{
    "name": "jeroenvanderlaan/2fa",
    "description": "A simple PHP 2FA implementation",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "endroid/qr-code": "^3.9",
        "php-di/php-di": "^6.3",
        "pragmarx/google2fa": "^8.0",
        "symfony/console": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        }
    }
}
